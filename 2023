			}
		}

	if(Hospital="ROH Trauma")
	{
		ROHTrauma := true
		HospitalNum := 1
		VA := ROH := MLH := false
		Guicontrol,2:show,EMR
		Guicontrol,2:show,PACS
		Guicontrol,2:hide,Office
