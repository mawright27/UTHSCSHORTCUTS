		Guicontrol,2:show,Office
		Guicontrol,2:hide,PACS

	}

	if(Hospital="VA")
	{
		ROH := ROHTrauma := MLH := false
		VA := true
		HospitalNum := 4
		Guicontrol,2:hide,EMR
		Guicontrol,2:show,Office
		Guicontrol,2:hide,PACS

	}
	


