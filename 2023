#a:: 

	WinGetClass, PrevActiveClass, A
	WinGetTitle, PrevActiveTitle, A

	if WinExist("Add ahk_exe anki.exe")
	{
	WinActivate
	Return
	}

	if WinExist("Browse ahk_exe anki.exe")
	{
	WinActivate
	send, {ctrl down}e{ctrl up}
	sleep, 200

	WinActivate, %PrevActiveTitle% Ahk_class %PrevActiveClass%
	WinActivate, Add ahk_exe anki.exe

	Return
	}

	else
	{
	WinActivate, User 1 - Anki ahk_exe anki.exe
	sleep, 100
	send, b
	sleep, 100
	send, {ctrl down}e{ctrl up}
	sleep, 200

	WinActivate, %PrevActiveTitle% Ahk_class %PrevActiveClass%
	WinActivate, Add ahk_exe anki.exe
	}
	return
	
;::::::::::::::::DATE FUNCTIONS::::::::::::::::

^numpad0::
	datevar1 := +0
	goto, Dateshortcut
^numpad1::
	datevar1 := +1
	goto, Dateshortcut
^numpad2::
	datevar1 := +2
	goto, Dateshortcut
^numpad3::
	datevar1 := +3
	goto, Dateshortcut
^numpad7::
	datevar1 := -1
	goto, Dateshortcut
^numpad8::
	datevar1 := -2
	goto, Dateshortcut
^numpad9::
	datevar1 := -3
	goto, Dateshortcut

DateShortcut:
	today = %a_now%
	today += %datevar1%, days
	FormatTime, today, %today%, M/d
	sendinput %today%
	Return

:*?:todaysdate::
^!numpad0::
	datevar1 := +0
	goto, DateshortcutLong
^!numpad1::
	datevar1 := +1
	goto, DateshortcutLong
^!numpad2::
	datevar1 := +2
	goto, DateshortcutLong
^!numpad3::
	datevar1 := +3
	goto, DateshortcutLong
^!numpad7::
	datevar1 := -1
	goto, DateshortcutLong
^!numpad8::
	datevar1 := -2
	goto, DateshortcutLong
^!numpad9::
	datevar1 := -3
	goto, DateshortcutLong

DateShortcutLong:
	today = %a_now%
	today += %datevar1%, days
	FormatTime, today, %today%, M/d/yy
	sendinput %today%
	Return

