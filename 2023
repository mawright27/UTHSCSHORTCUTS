WM_LBUTTONDOWN() {
	mousegetpos, OptionsXloc, Optionsyloc

	if(optionsxloc > 181 && optionsxloc < 278 && optionsyloc < 25)

;	msgbox, %OptionsXloc% , %Optionsyloc%

 	PostMessage, 0xA1, 2,,,A
	}
	return

~LButton::
    If (A_ThisHotkey = A_PriorHotkey and A_TimeSincePriorHotkey < 200)
	{
	ifwinactive UTHSC Shortcuts
		{
		mousegetpos, OptionsXloc, Optionsyloc
		if(optionsxloc > 181 && optionsxloc < 278 && optionsyloc < 25)
			{
			Gui, 1: submit, Nohide
			Gui, 1: Destroy
			goto, options
			return
			}
		}
	}
	Return

