~MButton::
	if(A_ThisHotKey = A_PriorHotKey) && (A_TimeSincePriorHotkey < 400) 
	{
		Gui, 2: Submit, NoHide
		clipboard := Attending
		Send, ^v
	}
	return

^+a::
	counthotkeys := counthotkeys +1
	IfNotExist, OldList.txt
		{
		GoTo,Options
		}
	
	ifExist, OldList.txt
		{
		FileRead,Temp_OldData1,OldList.txt
		Loop,Parse,Temp_OldData1,~
			{
				UserPass%A_Index% := A_LoopField
			}
		Sendraw, %UserPass1%
		Send,{TAB} 
		Sleep, 200
		Sendraw, %UserPass2%
		Send,{ENTER}
		}
	Return

