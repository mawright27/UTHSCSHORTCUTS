^numpad0::
	counthotkeys := counthotkeys +1
	datevar1 := +0
	goto, Dateshortcut
^numpad1::
	counthotkeys := counthotkeys +1
	datevar1 := +1
	goto, Dateshortcut
^numpad2::
	counthotkeys := counthotkeys +1
	datevar1 := +2
	goto, Dateshortcut
^numpad3::
	counthotkeys := counthotkeys +1
	datevar1 := +3
	goto, Dateshortcut
^numpad7::
	counthotkeys := counthotkeys +1
	datevar1 := -1
	goto, Dateshortcut
^numpad8::
	counthotkeys := counthotkeys +1
